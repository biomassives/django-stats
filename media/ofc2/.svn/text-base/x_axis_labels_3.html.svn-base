	<h2>X Axis Labels 3</h2>
	<div id="chart_x_axis_labels_3"></div>
	<div id="doc"><p>X Axis labels complex example</p>
</div>
	<pre><div class="syntax"><pre><span class="k">def</span> <span class="nf">test_x_axis_labels_3</span><span class="p">():</span>
    
    <span class="n">t</span> <span class="o">=</span> <span class="n">title</span><span class="p">(</span><span class="n">text</span><span class="o">=</span><span class="s">&#39;X Axis Labels Complex Example&#39;</span><span class="p">)</span>
    <span class="n">l</span> <span class="o">=</span> <span class="n">line_dot</span><span class="p">()</span>
    <span class="n">l</span><span class="o">.</span><span class="n">values</span> <span class="o">=</span> <span class="nb">range</span><span class="p">(</span><span class="mf">9</span><span class="p">,</span><span class="mf">0</span><span class="p">,</span><span class="o">-</span><span class="mf">1</span><span class="p">)</span>
    
    <span class="n">x</span> <span class="o">=</span> <span class="n">x_axis</span><span class="p">()</span>
    <span class="n">x</span><span class="o">.</span><span class="n">stroke</span> <span class="o">=</span> <span class="mf">1</span>
    <span class="n">x</span><span class="o">.</span><span class="n">colour</span> <span class="o">=</span> <span class="s">&#39;#428C3E&#39;</span>
    <span class="n">x</span><span class="o">.</span><span class="n">tick_height</span> <span class="o">=</span> <span class="mf">5</span>
    <span class="n">x</span><span class="o">.</span><span class="n">steps</span> <span class="o">=</span> <span class="mf">1</span>
    <span class="n">x</span><span class="o">.</span><span class="n">grid_colour</span> <span class="o">=</span> <span class="s">&#39;#86BF83&#39;</span>
    <span class="n">xlbls</span> <span class="o">=</span> <span class="n">x_axis_labels</span><span class="p">(</span><span class="n">steps</span><span class="o">=</span><span class="mf">2</span><span class="p">,</span> <span class="n">rotate</span><span class="o">=</span><span class="s">&#39;vertical&#39;</span><span class="p">,</span> <span class="n">colour</span><span class="o">=</span><span class="s">&#39;#FF0000&#39;</span><span class="p">,</span> <span class="n">size</span><span class="o">=</span><span class="mf">16</span><span class="p">)</span>

    <span class="n">lbls</span> <span class="o">=</span> <span class="p">[</span><span class="s">&#39;one&#39;</span><span class="p">,</span> <span class="s">&#39;two&#39;</span><span class="p">,</span> <span class="s">&#39;three&#39;</span><span class="p">,</span> <span class="s">&#39;four&#39;</span><span class="p">,</span> <span class="s">&#39;five&#39;</span><span class="p">]</span>
    <span class="n">lbls</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">x_axis_label</span><span class="p">(</span><span class="n">text</span><span class="o">=</span><span class="s">&#39;six&#39;</span><span class="p">,</span> <span class="n">colour</span><span class="o">=</span><span class="s">&#39;#0000FF&#39;</span><span class="p">,</span> <span class="n">size</span><span class="o">=</span><span class="mf">30</span><span class="p">,</span> <span class="n">rotate</span><span class="o">=</span><span class="s">&#39;vertical&#39;</span><span class="p">))</span>
    <span class="n">lbls</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">x_axis_label</span><span class="p">(</span><span class="n">text</span><span class="o">=</span><span class="s">&#39;seven&#39;</span><span class="p">,</span> <span class="n">colour</span><span class="o">=</span><span class="s">&#39;#0000FF&#39;</span><span class="p">,</span> <span class="n">size</span><span class="o">=</span><span class="mf">30</span><span class="p">,</span> <span class="n">rotate</span><span class="o">=</span><span class="s">&#39;vertical&#39;</span><span class="p">))</span>
    <span class="n">eight</span> <span class="o">=</span> <span class="n">x_axis_label</span><span class="p">(</span><span class="n">text</span><span class="o">=</span><span class="s">&#39;eight&#39;</span><span class="p">,</span> <span class="n">colour</span><span class="o">=</span><span class="s">&#39;#8C773E&#39;</span><span class="p">,</span> <span class="n">size</span><span class="o">=</span><span class="mf">16</span><span class="p">,</span> <span class="n">rotate</span><span class="o">=</span><span class="s">&#39;diagonal&#39;</span><span class="p">)</span>
    <span class="n">eight</span><span class="o">.</span><span class="n">visible</span> <span class="o">=</span> <span class="bp">True</span>
    <span class="n">lbls</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">eight</span><span class="p">)</span>
    <span class="n">lbls</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">x_axis_label</span><span class="p">(</span><span class="n">text</span><span class="o">=</span><span class="s">&#39;nine&#39;</span><span class="p">,</span> <span class="n">colour</span><span class="o">=</span><span class="s">&#39;#2683CF&#39;</span><span class="p">,</span> <span class="n">size</span><span class="o">=</span><span class="mf">16</span><span class="p">,</span> <span class="n">rotate</span><span class="o">=</span><span class="s">&#39;horizontal&#39;</span><span class="p">))</span>
    <span class="n">xlbls</span><span class="o">.</span><span class="n">labels</span> <span class="o">=</span> <span class="n">lbls</span>
    <span class="n">x</span><span class="o">.</span><span class="n">labels</span> <span class="o">=</span> <span class="n">xlbls</span>
    <span class="n">chart</span> <span class="o">=</span> <span class="n">open_flash_chart</span><span class="p">()</span>
    <span class="n">chart</span><span class="o">.</span><span class="n">title</span> <span class="o">=</span> <span class="n">t</span>
    <span class="n">chart</span><span class="o">.</span><span class="n">add_element</span><span class="p">(</span><span class="n">l</span><span class="p">)</span>
    <span class="n">chart</span><span class="o">.</span><span class="n">x_axis</span> <span class="o">=</span> <span class="n">x</span>
    <span class="k">return</span> <span class="n">chart</span>
</pre></div>
</pre> 
